<?xml version="1.0" encoding="utf-8"?>
<layout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
    </data>

    <RelativeLayout
            android:id="@+id/time_table_page_activity_background_RelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
    >
        <RelativeLayout
                android:id="@+id/time_table_page_activity_timeTableLayout_RelativeLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true">
            <TextView
                    android:id="@+id/time_table_page_activity_axis_TextView"
                    android:text="교시"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="1"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_below="@+id/time_table_page_activity_axis_TextView"
                    android:id="@+id/time_tablepage_activity_1_TextView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="2"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_below="@+id/time_tablepage_activity_1_TextView"
                    android:id="@+id/time_tablepage_activity_2_TextView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="3"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_below="@+id/time_tablepage_activity_2_TextView"
                    android:id="@+id/time_tablepage_activity_3_TextView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="4"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_below="@+id/time_tablepage_activity_3_TextView"
                    android:id="@+id/time_tablepage_activity_4_TextView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="5"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_below="@+id/time_tablepage_activity_4_TextView"
                    android:id="@+id/time_tablepage_activity_5_TextView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="6"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_below="@+id/time_tablepage_activity_5_TextView"
                    android:id="@+id/time_tablepage_activity_6_TextView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="7"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:layout_below="@+id/time_tablepage_activity_6_TextView"
                    android:id="@+id/time_tablepage_activity_7_TextView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>

            <TextView
                    android:text="월"
                    android:layout_toRightOf="@+id/time_table_page_activity_axis_TextView"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:id="@+id/time_table_page_activity_mon_TextView"
                    android:textStyle="bold"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="화"
                    android:layout_toRightOf="@+id/time_table_page_activity_mon_TextView"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:id="@+id/time_table_page_activity_tue_TextView"
                    android:textStyle="bold"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="수"
                    android:layout_toRightOf="@+id/time_table_page_activity_tue_TextView"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:id="@+id/time_table_page_activity_wed_TextView"
                    android:textStyle="bold"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="목"
                    android:layout_toRightOf="@+id/time_table_page_activity_wed_TextView"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:id="@+id/time_table_page_activity_thu_TextView"
                    android:textStyle="bold"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
            <TextView
                    android:text="금"
                    android:layout_toRightOf="@+id/time_table_page_activity_thu_TextView"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:id="@+id/time_table_page_activity_fri_TextView"
                    android:textStyle="bold"
                    android:layout_width="60dp"
                    android:layout_height="60dp"/>
        </RelativeLayout>

    </RelativeLayout>
</layout>
